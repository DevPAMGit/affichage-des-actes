<?xml version="1.0" ?>
<alfresco-config>
	<config evaluator="string-compare" condition="DocumentLibrary">
		<aspects>
			<visible>
				<aspect name="acme:securityClassified"/>
				<aspect name="actes59:docinfos"/>
				<aspect name="actes59:dossierinfos"/>
				<aspect name="stockageactes59:dossierInfo"/>
				<aspect name="erreur59:erreurBase"/>
				<aspect name="erreur59:erreurDossier"/>
			</visible>
			<addable> 
            </addable>
			<removeable> 
            </removeable>
		</aspects>
		<types>
			<type name="cm:content">
				<subtype name="acme:document"/>
				<subtype name="actes59:doc"/>
			</type>
			<type name="cm:folder">
				<subtype name="actes59:sas"/>
				<subtype name="actes59:dossier"/>
				<subtype name="stockageactes59:actesConteneur"/>
				<subtype name="stockageactes59:conteneurActeAnnee"/>
			</type>
		</types>
	</config>
	<config evaluator="string-compare" condition="AdvancedSearch" replace="true">
		<advanced-search>
			<forms>
				<form labelId="search.form.label.cm_content" descriptionId="search.form.desc.cm_content">cm:content
                </form>
				<form labelId="search.form.label.cm_folder" descriptionId="search.form.desc.cm_folder">cm:folder</form>
				<form labelId="form.label.advancedsearch.acmeDocument" descriptionId="form.description.advancedsearch.acmeDocument">acme:document
                </form>
			</forms>
		</advanced-search>
	</config>
	<config evaluator="node-type" condition="acme:document">
		<forms>
			<form>
				<field-visibility>
					<show id="acme:documentId"/>
					<show id="cm:name"/>
					<show id="cm:title" force="true"/>
					<show id="cm:description" force="true"/>
					<show id="cm:created" force="true"/>
					<show id="cm:modifier" force="true"/>
					<show id="cm:modified" force="true"/>
					<show id="cm:creator" for-mode="view"/>
				</field-visibility>
			</form>
		</forms>
	</config>
	<config evaluator="aspect" condition="acme:securityClassified">
